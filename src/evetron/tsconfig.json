{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "bin",
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": true,
    "preserveConstEnums": true
  },
  "exclude": [
    "bin",
    "node_modules",
    "typings"
  ]
}